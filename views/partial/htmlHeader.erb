<link rel="shortcut icon" href="/favicon.ico"/>
<link rel="bookmark" href="/favicon.ico"/>
<!--MiniUI css-->
<link href="/css/default/miniui.css" rel="stylesheet" type="text/css" />
<link href="/css/icons.css" rel="stylesheet" type="text/css" />
<link href="/css/ext.css" rel="stylesheet" type="text/css" />
<% QString skin=httpRequest().cookie("miniuiSkin");%>
<% QString mode=httpRequest().cookie("miniuiMode");%>
<% if (!skin.isEmpty()){ %>
   <%== styleSheetTag(cssPath(skin+"/skin.css"), a("id", "miniuiSkin")); %>
<% } %>
<% if (!mode.isEmpty()){ %>
   <%== styleSheetTag(cssPath(mode.prepend("default/")+"-mode.css"), a("id", "miniuiMode")); %>
<% } %>
<!--jQuery js-->
<script src="/js/jquery-3.1.1.min.js" type="text/javascript"></script>
<!--MiniUI js-->
<script type="text/javascript">miniui_debugger = false;</script>
 <script src="/js/miniui.js" type="text/javascript"></script>
<% QString lang=httpRequest().cookie("lang");%>
 <script src="/js/<%== lang=="zh"?"zh_CN":"en_US";%>.js" type="text/javascript" id="miniuiLang"></script>
 <script src="/js/core.js" type="text/javascript"></script> 
 <script src="/js/multisort.js" type="text/javascript"></script> 